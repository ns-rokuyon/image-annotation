<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered">
            <% @annotations.each do |anno| %>
                <tr>
                    <td><a href="<%= route_link(anno.route) %>"><%= anno.name %></a></td>
                    <td><%= anno.description %></td>
                </tr>
            <% end %>
        </table>
    </div>
</div>

